{
  "name": "rtl433-webmon",
  "version": "1.0.0",
  "description": "web front end for a software defined radio reading sensors",
  "main": "index.js",
  "dependencies": {
    "express": "^4.16.4",
    "forever": "^0.15.3",
    "serialport": "^5.0.0",
    "socket.io": "^1.7.4"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.5.3",
    "sinon": "^1.17.4",
    "standard": "^10.0.3"
  },
  "scripts": {
    "start": "node index.js",
    "daemon-start": "forever start --minUptime 1000 --spinSleepTime 1000 index.js",
    "daemon-stop": "forever stop index.js",
    "pretest": "standard",
    "test": "mocha test/**/*-test.js",
    "debug": "DEBUG=webmon* npm start"
  },
  "standard": {
    "globals": [
      "angular",
      "io"
    ],
    "envs": [
      "mocha"
    ]
  },
  "author": "Mark Frost",
  "license": "GPL-3.0",
  "repository": "https://github.com/frostmar/rtl433-webmon.git"
}
